"deltastep-screenshot"
	Shift + Alt + 3

"deltastep-screenshot -w"
	Shift + Alt + 4

#"amixer set Master unmute; amixer set Master 10%-"
"pamixer -u -d 10"
	XF86AudioLowerVolume

#"amixer set Master unmute; amixer set Master 10%+"
"pamixer -u -i 10"
	XF86AudioRaiseVolume

#"amixer set Master toggle"
"pamixer -t"
	XF86AudioMute

#"dm-tool lock"
"loginctl lock-session"
	XF86ScreenSaver

"deltastep-space left"
	Mod4 + Left

"deltastep-space right"
	Mod4 + Right

"deltastep-space new"
	Mod4 + N

"deltastep-space left_with_window"
	Shift + Mod4 + Left

"deltastep-space right_with_window"
	Shift + Mod4 + Right
